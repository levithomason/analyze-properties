  "scripts": {
    "build": "yarn build:extension && yarn build:mobile && yarn build:ui && yarn build:web",
    "build:extension": "NODE_ENV=production node scripts/genesis build extension",
    "build:mobile": "NODE_ENV=production node scripts/genesis build mobile",
    "build:ui": "NODE_ENV=production node scripts/genesis build ui",
    "build:web": "rm -rf ./dist/web && NODE_ENV=production node scripts/genesis build web",
    "firebase:backup": "yarn run firebase:login && node scripts/firebase-backup",
    "firebase:deploy": "yarn run build:web && cd dist/web && firebase deploy && cd ../..",
    "firebase:login": "firebase login",
    "lint": "prettier --config ./.prettierrc.js --write \"**/*.js?(on|x)\" && eslint --fix \"**/*.js?(on|x)\"",
    "prebuild:extension": "yarn run -s satisfied",
    "prebuild:mobile": "yarn run -s satisfied",
    "prebuild:ui": "yarn run -s satisfied",
    "prebuild:web": "yarn run -s satisfied",
    "prestart:extension": "yarn run -s satisfied",
    "prestart:mobile": "yarn run -s satisfied",
    "prestart:ui": "yarn run -s satisfied",
    "prestart:web": "yarn run -s satisfied",
    "satisfied": "which satisfied && satisfied --fix yarn --skip-invalid || echo \"satisfied is not installed\"",
    "start:extension": "node scripts/genesis start extension",
    "start:mobile": "node scripts/genesis start mobile",
    "start:ui": "node scripts/genesis start ui",
    "start:web": "node scripts/genesis start web",
    "test:extension": "NODE_ENV=test node scripts/genesis test extension"
  }
