    "fela-plugin-dynamic-prefixer": "^5.0.0",
    "fela-plugin-fallback-value": "^10.0.2",
    "fela-plugin-friendly-pseudo-class": "^10.0.2",
    "fela-plugin-important": "^5.0.7",
    "fela-plugin-placeholder-prefixer": "^10.0.2",
    "fela-plugin-prefixer": "^10.0.2",
    "fela-plugin-unit": "^10.0.2",
    "firebase": "^5.7.2",
    "font-awesome": "^4.7.0",
    "hoist-non-react-statics": "^2.2.2",
    "keyboard-key": "^1.0.1",
    "lit-html": "^1.1.0",
    "lodash": "^4.17.4",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-router5": "^4.2.1",
    "morgan": "^1.9.0",
    "normalize.css": "^7.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-fela": "^10.0.2",
